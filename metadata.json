{
  "name": "puppet-pkgng",
  "version": "2.4.1-rc0",
  "author": "Vox Pupuli",
  "summary": "PkgNG package provider for FreeBSD",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-pkgng.git",
  "project_page": "https://github.com/voxpupuli/puppet-pkgng",
  "issues_url": "https://github.com/voxpupuli/puppet-pkgng/issues",
