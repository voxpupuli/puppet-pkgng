# File managed by Puppet

<%= @name -%>: {
    url:         "<% if @mirror_type =~ /^srv$/i %>pkg+<% end %><%= @protocol %>://<%= @packagehost %><%= @repopath %>",
    mirror_type: "<%= @mirror_type %>",
    enabled:     <% if @enabled == true or @enabled == 'yes' %>yes<% else %>no<% end %>,
    priority:    <%= @priority %>
}
